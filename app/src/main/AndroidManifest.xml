<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hackingbuzz.databaseloginsignup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.hackingbuzz.database2loginsignup1.activites.activites.LoginScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hackingbuzz.database2loginsignup1.activites.activites.RegisterScreen"/>

        <activity
            android:name="com.hackingbuzz.database2loginsignup1.activites.activites.LeftSlideNavigation"
            android:label="@string/title_activity_left_slide_navigation"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>